<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="tk.Frame" id="master">
    <property name="height">450</property>
    <property name="width">600</property>
    <layout>
      <property name="column">0</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
      <rows>
        <row id="0">
          <property name="minsize">400</property>
          <property name="weight">1</property>
        </row>
        <row id="1">
          <property name="minsize">50</property>
          <property name="pad">10</property>
          <property name="weight">1</property>
        </row>
      </rows>
      <columns>
        <column id="0">
          <property name="minsize">600</property>
          <property name="weight">1</property>
        </column>
      </columns>
    </layout>
    <child>
      <object class="pygubu.builder.widgets.scrolledframe" id="scrolledframe">
        <property name="scrolltype">vertical</property>
        <property name="usemousewheel">true</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
          <property name="sticky">nsew</property>
          <rows>
            <row id="0">
              <property name="minsize">400</property>
              <property name="pad">10</property>
              <property name="weight">1</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="minsize">300</property>
              <property name="weight">1</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="tk.Text" id="text_entry">
            <property name="height">10</property>
            <property name="insertunfocussed">none</property>
            <property name="maxundo">0</property>
            <property name="state">disabled</property>
            <property name="width">40</property>
            <property name="wrap">word</property>
            <layout>
              <property name="column">0</property>
              <property name="columnspan">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="rowspan">1</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="input_frame">
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nsw</property>
          <rows>
            <row id="0">
              <property name="weight">1</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="minsize">50</property>
              <property name="weight">1</property>
            </column>
            <column id="1">
              <property name="minsize">100</property>
              <property name="weight">1</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Label" id="training_lbl">
            <property name="anchor">w</property>
            <property name="padding">5</property>
            <property name="text" translatable="yes">Training Path</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="pygubu.builder.widgets.pathchooserinput" id="training_input">
            <property name="state">normal</property>
            <property name="textvariable">string:training_input</property>
            <property name="type">file</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Frame" id="button_frame">
        <property name="height">50</property>
        <property name="padx">10</property>
        <property name="width">100</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">nse</property>
          <rows>
            <row id="0">
              <property name="weight">1</property>
            </row>
          </rows>
          <columns>
            <column id="0">
              <property name="minsize">50</property>
              <property name="pad">5</property>
              <property name="weight">1</property>
            </column>
            <column id="1">
              <property name="minsize">50</property>
              <property name="pad">5</property>
              <property name="weight">1</property>
            </column>
          </columns>
        </layout>
        <child>
          <object class="ttk.Button" id="train_btn">
            <property name="state">normal</property>
            <property name="text" translatable="yes">Train</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="gen_btn">
            <property name="state">normal</property>
            <property name="text" translatable="yes">Generate</property>
            <layout>
              <property name="column">1</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
  <object class="tk.Menu" id="mainmenu">
    <child>
      <object class="tk.Menuitem.Submenu" id="file_menu">
        <property name="label" translatable="yes">File</property>
        <property name="state">normal</property>
        <child>
          <object class="tk.Menuitem.Command" id="exit_btn">
            <property name="command">on_file_item_clicked</property>
            <property name="command_id_arg">true</property>
            <property name="compound">top</property>
            <property name="label" translatable="yes">Exit</property>
            <property name="state">normal</property>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="tk.Menuitem.Submenu" id="help_menu">
        <property name="label" translatable="yes">Help</property>
        <property name="state">normal</property>
        <child>
          <object class="tk.Menuitem.Command" id="about_btn">
            <property name="command">on_help_item_clicked</property>
            <property name="command_id_arg">true</property>
            <property name="label" translatable="yes">About</property>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
